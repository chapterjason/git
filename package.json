{
  "name": "@mscs/git",
  "version": "0.1.0-DEV",
  "description": "Interact programmatically with git",
  "repository": {
    "type": "git",
    "url": "git@github.com:media-service-dev/git.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "build:dev": "webpack --config webpack.config.js --mode=development",
    "build:watch": "webpack --config webpack.config.js --watch --mode=development",
    "lint": "eslint \"./src/**/*\" \"./__tests__/**/*\"",
    "test": "jest --runInBand",
    "docs": "typedoc --listInvalidSymbolLinks src/"
  },
  "files": [
    "dist/",
    "docs/",
    "yarn.lock",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@mscs/process": "^0.3.0"
  },
  "devDependencies": {
    "@mscs/eslint-config": "^0.1.2",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.12.50",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.1.0",
    "jest-each": "^26.1.0",
    "ts-jest": "^26.1.2",
    "ts-loader": "^8.0.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  }
}
